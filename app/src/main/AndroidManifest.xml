<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.alfinur.doctors" >

    <application
        android:allowBackup="true"
        android:icon="@drawable/clinic"
        android:label="@string/app_name"
        android:theme="@style/MyTheme"
        tools:ignore="NewApi">
        <activity
            android:name=".activities.DoctorsActivity"
            android:icon="@drawable/clinic"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".activities.SheduleActivity"
            android:label="@string/shedule" >
        </activity>
        <activity
            android:name=".activities.ArchiveActivity"
            android:label="@string/archive" >
        </activity>
    </application>

</manifest>
